                                        <input id="CompanyInput" asp-for="NewCustomer.CompanyName" class="form-control" />
                                        <span asp-validation-for="NewCustomer.CompanyName" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="form-row">
                            <div class="col-xs-10" id="ContactDetails">
