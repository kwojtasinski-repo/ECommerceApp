                                        <input id="AddressStreetInput" asp-for="NewCustomer.Addresses[0].Street" class="form-control" />
                                        <span asp-validation-for="NewCustomer.Addresses[0].Street" class="text-danger"></span>
                                    </div>
                                    <div>
